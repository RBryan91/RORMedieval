<h1>New Enigme</h1>

<%= form_with model: @enigme do |form| %>
  <div>
    <%= form.label :titre %><br>
    <%= form.text_field :titre %>
    <% @enigme.errors.full_messages_for(:titre).each do |message| %>
      <div><%= message %></div>
      <% end %>
  </div>

  <div>
    <%= form.submit 'Choice the question' %>
  </div>
<% end %>