<h1>Welcome Master</h1>

<%= form_tag(authenticateMaster_path, method: 'post') do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  
    <%= submit_tag 'Login' %>

  <% end %>

  <%= link_to "Register", new_master_path %>